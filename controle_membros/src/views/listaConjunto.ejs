<%- include('includes/head'); %>

    <div class="container col-6 py-5">
        <div class="card-header text-center">
            <h4>Conjuntos cadastrados</h4>
            <%- include('includes/messages'); %>
        </div>
        <table class="table">
            <% conjuntos.forEach(conjunto=> {%>
                <tr>
                    <td>
                        <%= conjunto.nome %>
                    </td>
                    <td><a class="text-primary" href="/conjunto/edit/<%= conjunto._id %>">Editar</a></td>
                    <td><a class="text-danger" href="/conjunto/delete/<%= conjunto._id %>">Excluir</a></td>
                </tr>
                <% }) %>
        </table>
        <a class="btn btn-primary" href="/conjunto/lista" role="button">Cadastrar</a>
        <a class="btn btn-secondary" href="/" role="button">Voltar</a>
    </div>

    <%- include('includes/footer'); %>